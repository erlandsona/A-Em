{
  "name": "A-Em",
  "version": "1.0.0",
  "description": "Band page for A&Em",
  "keywords": [
    "elm",
    "webpack"
  ],
  "author": "erlandsona <austin@erlandson.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:erlandsona/A-Em.git"
  },
  "license": "MIT",
  "main": "index.html",
  "dependencies": {
    "font-awesome": "^4.6.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "elm-css": "^0.5.1",
    "elm-css-webpack-loader": "^2.0.1",
    "elm-hot-loader": "^0.3.3",
    "elm-live": "^2.6.0",
    "elm-webpack-loader": "^3.0.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "postcss-loader": "^0.10.1",
    "rimraf": "^2.5.3",
    "shx": "^0.1.4",
    "style-loader": "^0.13.1",
    "unminified-webpack-plugin": "^1.1.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "scripts": {
    "clean": "rimraf dist node_modules elm-stuff",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build-ci": "elm-package install --yes && npm run build && npm run build-css",
    "build-css": "shx mkdir -p ./dist && elm-css build/Stylesheets.elm -o ./dist",
    "start": "elm-live src/Main.elm --output dist/app.js --dir=./dist --port=7777 --before-build=./build/buildCss",
    "elm-css": "elm-css build/Stylesheets.elm -o dist"
  }
}
