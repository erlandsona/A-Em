#!/usr/bin/env bash
set -e

./build/development/prestart

./build/development/tasks
# copy index.tpl.html to 404.html for "pushstate" imitation
cp build/index.tpl.html public/404.html

echo caldwell.band >> public/CNAME

# Compile elm-js
elm-make app/Caldwell.elm --output=public/bundle.js

# Send to gh-pages branch
./build/production/deploy

